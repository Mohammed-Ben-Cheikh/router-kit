{
  "name": "router-kit",
  "author": {
    "name": "Mohammed Ben Cheikh",
    "email": "mohammed.bencheikh.dev@gmail.com",
    "url": "https://mohammedbencheikh.com/"
  },
  "version": "2.0.1",
  "description": "A professional React routing library with guards, loaders, and navigation blocking",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./ssr": {
      "types": "./dist/ssr/index.d.ts",
      "import": "./dist/ssr/index.js",
      "require": "./dist/ssr/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -p tsconfig.json --watch",
    "prepare": "npm run build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "pack:verify": "npm pack --dry-run",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "validate:deps": "npm ls"
  },
  "dependencies": {
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "typescript": "^5.2.0"
  },
  "overrides": {
    "minimist": "^1.2.6",
    "node-fetch": "^2.6.7"
  },
  "keywords": [
    "react",
    "route",
    "provider",
    "kit",
    "routing",
    "route-provider",
    "route-kit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Mohammed-Ben-Cheikh/router-kit.git"
  },
  "bugs": {
    "url": "https://github.com/Mohammed-Ben-Cheikh/router-kit/issues"
  },
  "homepage": "https://github.com/Mohammed-Ben-Cheikh/router-kit#readme",
  "license": "MIT"
}
