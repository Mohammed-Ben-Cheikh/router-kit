{
  "name": "router-kit",
  "author": {
    "name": "Mohammed Ben Cheikh",
    "email": "mohammed.bencheikh.dev@gmail.com",
    "url": "https://mohammedbencheikh.com/"
  },
  "version": "1.2.4",
  "description": "A small React routing provider library",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup",
    "build:watch": "tsup --watch",
    "prepare": "npm run build",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "pack:verify": "npm pack --dry-run",
    "audit": "npm audit",
    "audit:fix": "npm audit fix",
    "validate:deps": "npm ls"
  },
  "peerDependencies": {
    "react": ">=16 <20",
    "react-dom": ">=16 <20"
  },
  "dependencies": {
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/url-join": "^5.0.0",
    "tsup": "8.5.0",
    "typescript": "^5.2.0"
  },
  "overrides": {
    "minimist": "^1.2.6",
    "node-fetch": "^2.6.7"
  },
  "keywords": [
    "react",
    "route",
    "provider",
    "kit",
    "routing",
    "route-provider",
    "route-kit"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Mohammed-Ben-Cheikh/router-kit.git"
  },
  "bugs": {
    "url": "https://github.com/Mohammed-Ben-Cheikh/router-kit/issues"
  },
  "homepage": "https://github.com/Mohammed-Ben-Cheikh/router-kit#readme",
  "license": "MIT"
}
